            <kind>1</kind>
            <string>/// Render background

// abort if nothing to draw
if back&lt;0 and sprite_index&lt;0 exit;
var sx, sy, lx, ly, rx, ry, cx, cy;

// get draw coordinates
var fx = x+floor(view_xview[view_current]*relative_x)+absolute_x;
var fy = y+floor(view_yview[view_current]*relative_y)+absolute_y;

// get separation
if back&gt;-1 {sx = width+separation_x; sy = height+separation_y;} else
{sx = sprite_width+separation_x; sy = sprite_height+separation_y;}

// get boundaries
if (tiled&amp;1) lx = view_xview[view_current]+((fx-view_xview[view_current]) mod sx)-sx; else lx = fx;
if (tiled&amp;2) ly = view_yview[view_current]+((fy-view_yview[view_current]) mod sy)-sy; else ly = fy;
if (tiled&amp;1) rx = view_xview[view_current]+view_wview[view_current]+sx; else rx = fx;
if (tiled&amp;2) ry = view_yview[view_current]+view_hview[view_current]+sy; else ry = fy;

// draw tiled
for (cy=ly; cy&lt;=ry; cy+=sy)
{
    for (cx=lx; cx&lt;=rx; cx+=sx)
    {
        if back&gt;-1 draw_background_part_ext(back, left, top, width, height, floor(cx), cy, image_xscale, image_yscale, image_blend, image_alpha); else
        draw_sprite_ext(sprite_index, image_index, floor(cx), cy, image_xscale, image_yscale, image_angle, image_blend, image_alpha);
    }
}
